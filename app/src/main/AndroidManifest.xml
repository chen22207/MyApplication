<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cs.myapplication">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:icon="@mipmap/logo"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RecyclerViewActivity" />
        <activity android:name=".ImageLoader.ImageLoaderActivity" />
        <activity android:name=".customAnimation.CustomAnimationActivity" />
        <activity android:name=".swipeRefreshLayout.SwipeRefreshLayoutActivity" />
        <activity android:name=".touchTest.TouchTestActivity" />
        <activity android:name=".customCamera.CustomCamera" />
        <activity android:name=".window.WindowTestActivity" />
        <activity android:name=".Db.DbActivity" />
        <activity android:name=".rxandroid.ImageGalleryActivity" />
        <activity android:name=".retrofit.RetrofitTest" />
        <activity android:name=".okhttp.OkhttpTestActivity" />
        <activity android:name=".capture.CaptureActivity" />
        <activity
            android:name=".rxandroid.NormalTest"
            android:label="@string/title_activity_normal_test"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".databinding.DataBindingActivity"
            android:label="@string/title_activity_data_binding"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
